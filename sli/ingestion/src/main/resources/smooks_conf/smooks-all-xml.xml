  <jb:bean beanId="otherNameType" class="java.util.HashMap" createOnElement="InterchangeStudent/Student">
    <jb:value property="otherNameType" data="Student/OtherName/@OtherNameType" />
    <jb:value property="personalTitlePrefixType" data="Student/OtherName/PersonalTitlePrefixType" />
    <jb:value property="firstName" data="Student/OtherName/FirstName" />
    <jb:value property="middleName" data="Student/OtherName/MiddleName" />
    <jb:value property="lastSurname" data="Student/OtherName/LastSurname" />
    <jb:value property="generationCodeSuffix" data="Student/OtherName/GenerationCodeSuffix" />
  </jb:bean>

  <!-- BirthDataType -->
  <jb:bean beanId="birthDataType" class="java.util.HashMap" createOnElement="InterchangeStudent/Student">
    <jb:value property="birthDate" data="Student/BirthData/BirthDate" />
    <jb:value property="cityOfBirth" data="Student/BirthData/CityOfBirth" />
    <jb:value property="stateOfBirthAbbreviation" data="Student/BirthData/StateOfBirthAbbreviation" />
    <jb:value property="countryOfBirthCode" data="Student/BirthData/CountryOfBirthCode" />
    <jb:value property="dateEnteredUS" data="Student/BirthData/DateEnteredUS" />
    <jb:value property="multipleBirthStatus" data="Student/BirthData/MultipleBirthStatus" decoder="Boolean" />
  </jb:bean>

  <!-- AddressType -->
  <jb:bean beanId="addressTypeArray" class="java.util.ArrayList" createOnElement="InterchangeStudent/Student">
    <jb:wiring beanIdRef="addressType" />
  </jb:bean>

  <jb:bean beanId="addressType" class="java.util.HashMap" createOnElement="InterchangeStudent/Student">
    <jb:value property="addressType" data="Student/Address/@AddressType" />
    <jb:value property="streetNumberName" data="Student/Address/StreetNumberName" />
    <jb:value property="apartmentRoomSuiteNumber" data="Student/Address/ApartmentRoomSuiteNumber" />
    <jb:value property="buildingSiteNumber" data="Student/Address/BuildingSiteNumber" />
    <jb:value property="city" data="Student/Address/City" />
    <jb:value property="stateAbbreviation" data="Student/Address/StateAbbreviation" />
    <jb:value property="postalCode" data="Student/Address/PostalCode" />
    <jb:value property="nameOfCounty" data="Student/Address/NameOfCounty" />
    <jb:value property="countyFIPSCode" data="Student/Address/CountyFIPSCode" />
    <jb:value property="countryCode" data="Student/Address/CountryCode" />
    <jb:value property="latitude" data="Student/Address/Latitude" />
    <jb:value property="longitude" data="Student/Address/Longitude" />
    <jb:value property="beginDate" data="Student/Address/BeginDate" />
    <jb:value property="endDate" data="Student/Address/EndDate" />
  </jb:bean>

  <!-- TelephoneType -->
  <jb:bean beanId="telephoneTypeArray" class="java.util.ArrayList" createOnElement="InterchangeStudent/Student">
    <jb:wiring beanIdRef="telephoneType" />
  </jb:bean>

  <jb:bean beanId="telephoneType" class="java.util.HashMap" createOnElement="InterchangeStudent/Student">
    <jb:value property="telephoneNumberType" data="Student/Telephone/@TelephoneNumberType" />
    <jb:value property="telephoneNumber" data="Student/Telephone/TelephoneNumber" />
    <jb:value property="primaryTelephoneNumberIndicator" data="Student/Telephone/@PrimaryTelephoneNumberIndicator"
      decoder="Boolean" />
  </jb:bean>

  <!-- ElectronicMailType -->
  <jb:bean beanId="electronicMailTypeArray" class="java.util.ArrayList" createOnElement="InterchangeStudent/Student">
    <jb:wiring beanIdRef="electronicMailType" />
  </jb:bean>

  <jb:bean beanId="electronicMailType" class="java.util.HashMap" createOnElement="InterchangeStudent/Student">
    <jb:value property="emailAddressType" data="Student/ElectronicMail/@EmailAddressType" />
    <jb:value property="emailAddress" data="Student/ElectronicMail/EmailAddress" />
  </jb:bean>

  <!-- RaceItemType ** -->
  <jb:bean beanId="raceItemTypeArray" class="java.util.ArrayList" createOnElement="InterchangeStudent/Student">
    <jb:value data="Student/Race/RacialCategory" />
  </jb:bean>


  <!-- StudentCharacteristicsType -->
  <jb:bean beanId="studentCharacteristicTypeArray" class="java.util.ArrayList" createOnElement="InterchangeStudent/Student">
    <jb:wiring beanIdRef="studentCharacteristicType" />
  </jb:bean>

  <jb:bean beanId="studentCharacteristicType" class="java.util.HashMap" createOnElement="InterchangeStudent/Student">
    <jb:value property="characteristic" data="Student/StudentCharacteristics/Characteristic" />
    <jb:value property="beginDate" data="Student/StudentCharacteristics/BeginDate" />
    <jb:value property="endDate" data="Student/StudentCharacteristics/EndDate" />
    <jb:value property="designatedBy" data="Student/StudentCharacteristics/DesignatedBy" />
  </jb:bean>

  <!-- LanguageItemType -->
  <jb:bean beanId="languageItemTypeArray" class="java.util.ArrayList" createOnElement="InterchangeStudent/Student">
    <jb:value data="Student/Languages/Language" />
  </jb:bean>

  <!-- HomeLanguageItemType -->
  <jb:bean beanId="homeLanguageItemTypeArray" class="java.util.ArrayList" createOnElement="InterchangeStudent/Student">
    <jb:value data="Student/HomeLanguages/Language" />
  </jb:bean>

  <!-- DisabilitiyType -->
  <jb:bean beanId="disabilityTypeArray" class="java.util.ArrayList" createOnElement="InterchangeStudent/Student">
    <jb:wiring beanIdRef="disabilityType" />
  </jb:bean>
  <jb:bean beanId="disabilityType" class="java.util.HashMap" createOnElement="InterchangeStudent/Student">
    <jb:value property="disability" data="Student/Disabilities/Disability" />
    <jb:value property="disabilityDiagnosis" data="Student/Disabilities/DisabilityDiagnosis" />
    <jb:value property="orderOfDisability" data="Student/Disabilities/OrderOfDisability" decoder="Integer" />
  </jb:bean>

  <!-- Section504DisabilitiesType -->
  <jb:bean beanId="section504DisabilitiesType" class="java.util.HashMap" createOnElement="InterchangeStudent/Student">
    <jb:wiring property="section504Disability" beanIdRef="section504DisabilitiesItemTypeArray" />
  </jb:bean>

  <jb:bean beanId="section504DisabilitiesItemTypeArray" class="java.util.ArrayList" createOnElement="InterchangeStudent/Student">
    <jb:value data="Student/Section504Disabilities/Section504Disability" />
  </jb:bean>

  <!-- ProgramParticipationType -->
  <jb:bean beanId="programParticipationTypeArray" class="java.util.ArrayList" createOnElement="InterchangeStudent/Student">
    <jb:wiring beanIdRef="programParticipationType" />
  </jb:bean>
  <jb:bean beanId="programParticipationType" class="java.util.HashMap" createOnElement="InterchangeStudent/Student">
    <jb:value property="program" data="Student/ProgramParticipations/Program" />
    <jb:value property="beginDate" data="Student/ProgramParticipations/BeginDate" />
    <jb:value property="endDate" data="Student/ProgramParticipations/EndDate" />
    <jb:value property="designatedBy" data="Student/ProgramParticipations/DesignatedBy" />
  </jb:bean>

  <!--LearningStylesType -->
  <jb:bean beanId="learningStylesType" class="java.util.HashMap" createOnElement="InterchangeStudent/Student">
    <jb:value property="visualLearning" data="Student/LearningStyles/VisualLearning" decoder="Integer" />
    <jb:value property="auditoryLearning" data="Student/LearningStyles/AuditoryLearning" decoder="Integer" />
    <jb:value property="tactileLearning" data="Student/LearningStyles/TactileLearning" decoder="Integer" />
  </jb:bean>

  <!-- CohortYearsType -->
  <jb:bean beanId="cohortYearTypeArray" class="java.util.ArrayList" createOnElement="InterchangeStudent/Student">
    <jb:wiring beanIdRef="cohortYearType" />
  </jb:bean>

  <jb:bean beanId="cohortYearType" class="java.util.HashMap" createOnElement="InterchangeStudent/Student">
    <jb:value property="cohortYearType" data="Student/CohortYears/@CohortYearType" />
    <jb:value property="year" data="Student/CohortYears/Year" decoder="Integer" />
  </jb:bean>

  <!--StudentIndicatorType -->
  <jb:bean beanId="studentIndicatorTypeArray" class="java.util.ArrayList" createOnElement="InterchangeStudent/Student">
    <jb:wiring beanIdRef="studentIndicatorType" />
  </jb:bean>

  <jb:bean beanId="studentIndicatorType" class="java.util.HashMap" createOnElement="InterchangeStudent/Student">
    <jb:value property="indicatorName" data="Student/StudentIndicators/IndicatorName" />
    <jb:value property="indicator" data="Student/StudentIndicators/Indicator" />
    <jb:value property="beginDate" data="Student/StudentIndicators/BeginDate" />
    <jb:value property="endDate" data="Student/StudentIndicators/EndDate" />
    <jb:value property="designatedBy" data="Student/StudentIndicators/DesignatedBy" />
  </jb:bean>

  <!-- School mapping -->

  <jb:bean beanId="record" class="org.slc.sli.ingestion.NeutralRecord" createOnElement="InterchangeEducationOrganization/School">

    <!-- jb:value property="sourceId" data="" / -->
    <!-- jb:value property="jobId" data="" / -->
    <jb:expression property="recordType">'school'</jb:expression>
    <jb:value property="localId" data="StateOrganizationId" />
    <jb:wiring property="attributes" beanIdRef="schoolAttributes" />
  </jb:bean>

  <jb:bean beanId="schoolAttributes" class="java.util.HashMap" createOnElement="InterchangeEducationOrganization/School">

    <jb:value property="stateOrganizationId" data="School/StateOrganizationId" />

    <!-- EducationOrgIdentificationCode -->
    <jb:wiring property="educationOrgIdentificationCode" beanIdRef="educationOrgIdentificationCodeTypeArray" />

    <!-- NameOfInstitution -->
    <jb:value property="nameOfInstitution" data="School/NameOfInstitution" />

    <!-- ShortNameOfInstitution -->
    <jb:value property="shortNameOfInstitution" data="School/ShortNameOfInstitution" />

    <!-- OrganizationCategories -->
    <jb:wiring property="organizationCategories" beanIdRef="educationOrganizationCategoryTypeArray" />

    <!-- Address -->
    <jb:wiring property="address" beanIdRef="addressSchoolTypeArray" />

    <!-- Telephone -->
    <jb:wiring property="telephone" beanIdRef="institutionTelephoneTypeArray" />

    <!-- WebSite -->
    <jb:value property="webSite" data="School/WebSite" />

    <!-- OperationalStatus -->
    <jb:value property="operationalStatus" data="School/OperationalStatus" />

    <!-- AccountabilityRatings -->
    <jb:wiring property="accountabilityRatings" beanIdRef="accountabilityRatingsTypeArray" />

    <!-- GradesOffered -->
    <jb:wiring property="gradesOffered" beanIdRef="gradesOfferedTypeArray" />

    <!-- SchoolCategories -->
    <jb:wiring property="schoolCategories" beanIdRef="schoolCategoriesTypeArray" />

    <!-- SchoolType -->
    <jb:value property="schoolType" data="School/SchoolType" />

    <!-- charterStatus -->
