     <jb:bean beanId="record" class="org.slc.sli.ingestion.NeutralRecord" createOnElement="InterchangeEducationOrganization/Section">

    <jb:expression property="recordType">'section'</jb:expression>
    <jb:value property="localId" data="UniqueSectionCode" />
