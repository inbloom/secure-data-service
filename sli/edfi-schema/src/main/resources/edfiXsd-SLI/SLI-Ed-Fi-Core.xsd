	<xs:include schemaLocation="../edfiXsd/Ed-Fi-Core.xsd"/>
	<xs:annotation>
		<xs:documentation>===== SLI Transactional Entity =====</xs:documentation>
	</xs:annotation>
	<xs:simpleType name="ActionTypeType">
		<xs:annotation>
			<xs:documentation>HTTP like method for update operation</xs:documentation>
		</xs:annotation>
		<xs:restriction base="xs:token">
			<xs:enumeration value="POST"/>
			<xs:enumeration value="PUT"/>
			<xs:enumeration value="DELETE"/>
			<xs:enumeration value="MOVE"/>
			<xs:enumeration value="PATCH"/>
		</xs:restriction>
	</xs:simpleType>
	<xs:complexType name="SLC-ActionType">
		<xs:annotation>
			<xs:documentation>This is the verb wrapper that surrounds a sequence of objects to be operated on.</xs:documentation>
		</xs:annotation>
		<xs:choice minOccurs="0" maxOccurs="unbounded">
			<!-- All reference types  -->
			<xs:element name="AssessmentFamilyReference" type="SLC-AssessmentFamilyReferenceType"/>
			<xs:element name="AssessmentItemReference" type="SLC-AssessmentItemReferenceType"/>
			<xs:element name="AssessmentPeriodDescriptorReference" type="SLC-AssessmentPeriodDescriptorReferenceType"/>
			<xs:element name="AssessmentReference" type="SLC-AssessmentReferenceType"/>
			<xs:element name="AttendanceEventReference" type="SLC-AttendanceEventReferenceType"/>
			<xs:element name="CalendarDateReference" type="SLC-CalendarDateReferenceType"/>
			<xs:element name="CohortReference" type="SLC-CohortReferenceType"/>
			<xs:element name="CompetencyLevelDescriptorReference" type="SLC-CompetencyLevelDescriptorReferenceType"/>
			<xs:element name="CourseOfferingReference" type="SLC-CourseOfferingReferenceType"/>
			<xs:element name="CourseReference" type="SLC-CourseReferenceType"/>
			<xs:element name="CourseTranscriptReference" type="SLC-CourseTranscriptReferenceType"/>
			<xs:element name="DisciplineActionReference" type="SLC-DisciplineActionReferenceType"/>
			<xs:element name="DisciplineIncidentReference" type="SLC-DisciplineIncidentReferenceType"/>
			<xs:element name="EducationOrganizationReference" type="SLC-EducationalOrgReferenceType"/>
			<xs:element name="GradebookEntryReference" type="SLC-GradebookEntryReferenceType"/>
			<xs:element name="GradeReference" type="SLC-GradeReferenceType"/>
			<xs:element name="GradingPeriodReference" type="SLC-GradingPeriodReferenceType"/>
			<xs:element name="GraduationPlanReference" type="SLC-GraduationPlanReferenceType"/>
			<xs:element name="LearningObjectiveReference" type="SLC-LearningObjectiveReferenceType"/>
			<xs:element name="LearningStandardReference" type="SLC-LearningStandardReferenceType"/>
			<xs:element name="ObjectiveAssessmentReference" type="SLC-ObjectiveAssessmentReferenceType"/>
			<xs:element name="ParentReference" type="SLC-ParentReferenceType"/>
			<xs:element name="ProgramReference" type="SLC-ProgramReferenceType"/>
			<xs:element name="ReportCardReference" type="SLC-ReportCardReferenceType"/>
			<xs:element name="SectionReference" type="SLC-SectionReferenceType"/>
			<xs:element name="SessionReference" type="SLC-SessionReferenceType"/>
			<xs:element name="StaffCohortAssociationReference" type="SLC-StaffCohortAssociationReferenceType"/>
			<xs:element name="StaffEducationOrgAssignmentAssociationReference" type="SLC-StaffEducationOrgAssignmentAssociationReferenceType"/>
			<xs:element name="StaffProgramAssociationReference" type="SLC-StaffProgramAssociationReferenceType"/>
			<xs:element name="StaffReference" type="SLC-StaffReferenceType"/>
			<xs:element name="StudentAcademicRecordReference" type="SLC-StudentAcademicRecordReferenceType"/>
			<xs:element name="StudentAssessmentReference" type="SLC-StudentAssessmentReferenceType"/>
			<xs:element name="StudentAssessmentItemReference" type="SLC-StudentAssessmentItemReferenceType"/>
			<xs:element name="StudentCohortAssociationReference" type="SLC-StudentCohortAssociationReferenceType"/>
			<xs:element name="StudentCompetencyObjectiveReference" type="SLC-StudentCompetencyObjectiveReferenceType"/>
			<xs:element name="StudentCompetencyReference" type="SLC-StudentCompetencyReferenceType"/>
			<xs:element name="StudentDisciplineIncidentAssociationReference" type="SLC-StudentDisciplineIncidentAssociationReferenceType"/>
			<xs:element name="StudentGradebookEntryReference" type="SLC-StudentGradebookEntryReferenceType"/>
			<xs:element name="StudentObjectiveAssessmentReference" type="SLC-StudentObjectiveAssessmentReferenceType"/>
			<xs:element name="StudentParentAssociationReference" type="SLC-StudentParentAssociationReferenceType"/>
			<xs:element name="StudentProgramAssociationReference" type="SLC-StudentProgramAssociationReferenceType"/>
			<xs:element name="StudentReference" type="SLC-StudentReferenceType"/>
			<xs:element name="StudentSchoolAssociationReference" type="SLC-StudentSchoolAssociationReferenceType"/>
			<xs:element name="StudentSectionAssociationReference" type="SLC-StudentSectionAssociationReferenceType"/>
			<xs:element name="TeacherReference" type="SLC-StaffReferenceType"/>
			<xs:element name="TeacherSchoolAssociationReference" type="SLC-TeacherSchoolAssociationReferenceType"/>
			<xs:element name="TeacherSectionAssociationReference" type="SLC-TeacherSectionAssociationReferenceType"/>
			<!-- All top level entity types from the Interchange schemas -->
			<xs:element name="Assessment" type="SLC-Assessment"/>
			<xs:element name="AssessmentFamily" type="SLC-AssessmentFamily"/>
			<xs:element name="AssessmentItem" type="SLC-AssessmentItem"/>
			<xs:element name="AssessmentPeriodDescriptor" type="SLC-AssessmentPeriodDescriptor"/>
			<xs:element name="AttendanceEvent" type="SLC-AttendanceEvent"/>
			<xs:element name="CalendarDate" type="SLC-CalendarDate"/>
			<xs:element name="Cohort" type="SLC-Cohort"/>
			<xs:element name="CompetencyLevelDescriptor" type="SLC-CompetencyLevelDescriptor"/>
			<xs:element name="Course" type="SLC-Course"/>
			<xs:element name="CourseOffering" type="SLC-CourseOffering"/>
			<xs:element name="CourseTranscript" type="SLC-CourseTranscript"/>
			<xs:element name="DisciplineAction" type="SLC-DisciplineAction"/>
			<xs:element name="DisciplineIncident" type="SLC-DisciplineIncident"/>
			<xs:element name="EducationOrganization" type="SLC-EducationOrganization"/>
			<xs:element name="Grade" type="SLC-Grade"/>
			<xs:element name="GradebookEntry" type="SLC-GradebookEntry"/>
			<xs:element name="GradingPeriod" type="SLC-GradingPeriod"/>
			<xs:element name="GraduationPlan" type="SLC-GraduationPlan"/>
			<xs:element name="LearningObjective" type="SLC-LearningObjective"/>
			<xs:element name="LearningStandard" type="SLC-LearningStandard"/>
			<xs:element name="ObjectiveAssessment" type="SLC-ObjectiveAssessment"/>
			<xs:element name="Parent" type="SLC-Parent"/>
			<xs:element name="Program" type="SLC-Program"/>
			<xs:element name="ReportCard" type="SLC-ReportCard"/>
			<xs:element name="Section" type="SLC-Section"/>
			<xs:element name="Session" type="SLC-Session"/>
			<xs:element name="Staff" type="SLC-Staff"/>
			<xs:element name="StaffCohortAssociation" type="SLC-StaffCohortAssociation"/>
			<xs:element name="StaffEducationOrgAssignmentAssociation" type="SLC-StaffEducationOrgAssignmentAssociation"/>
			<xs:element name="StaffProgramAssociation" type="SLC-StaffProgramAssociation"/>
			<xs:element name="Student" type="SLC-Student"/>
			<xs:element name="StudentAcademicRecord" type="SLC-StudentAcademicRecord"/>
			<xs:element name="StudentAssessment" type="SLC-StudentAssessment"/>
			<xs:element name="StudentAssessmentItem" type="SLC-StudentAssessmentItem"/>
			<xs:element name="StudentCohortAssociation" type="SLC-StudentCohortAssociation"/>
			<xs:element name="StudentCompetency" type="SLC-StudentCompetency"/>
			<xs:element name="StudentCompetencyObjective" type="SLC-StudentCompetencyObjective"/>
			<xs:element name="StudentDisciplineIncidentAssociation" type="SLC-StudentDisciplineIncidentAssociation"/>
			<xs:element name="StudentGradebookEntry" type="SLC-StudentGradebookEntry"/>
			<xs:element name="StudentObjectiveAssessment" type="SLC-StudentObjectiveAssessment"/>
			<xs:element name="StudentParentAssociation" type="SLC-StudentParentAssociation"/>
			<xs:element name="StudentProgramAssociation" type="SLC-StudentProgramAssociation"/>
			<xs:element name="StudentSchoolAssociation" type="SLC-StudentSchoolAssociation"/>
			<xs:element name="StudentSectionAssociation" type="SLC-StudentSectionAssociation"/>
			<xs:element name="Teacher" type="SLC-Teacher"/>
			<xs:element name="TeacherSchoolAssociation" type="SLC-TeacherSchoolAssociation"/>
			<xs:element name="TeacherSectionAssociation" type="SLC-TeacherSectionAssociation"/>
		</xs:choice>
		<xs:attribute name="ActionType" type="ActionTypeType" use="optional">
			<xs:annotation>
				<xs:documentation>The HTTP method indicating the type of operation being done on the object.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Cascade" type="xs:boolean" use="optional">
			<xs:annotation>
				<xs:documentation>Whether to cascade update or delete operation.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="Force" type="xs:boolean" use="optional">
			<xs:annotation>
				<xs:documentation>Whether to ignore referential integrity violations during delete operations.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="LogViolations" type="xs:boolean" use="optional">
			<xs:annotation>
				<xs:documentation>For Force delete operations, whether to report objects that will be orphaned by the delete.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<xs:attribute name="MaxObjects" type="xs:integer" use="optional">
			<xs:annotation>
				<xs:documentation>Maximum number of total objects allowed to be affected by the operation. If absent, use system default. If zero, unbounded.</xs:documentation>
			</xs:annotation>
		</xs:attribute>
		<!-- Sequence of entities wrapped -->
		<!--  Attributes -->
	</xs:complexType>
	<xs:annotation>
		<xs:documentation>===== Domain Entities =====</xs:documentation>
	</xs:annotation>
	<xs:complexType name="SLC-AcademicWeek">
		<xs:annotation>
			<xs:documentation>Changed type of CalendarDateReference to SLC reference type.</xs:documentation>
			<xs:appinfo>
				<sli:recordType>academicWeek</sli:recordType>
			</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="ComplexObjectType">
				<xs:sequence>
					<xs:element name="WeekIdentifier" type="WeekIdentifier"/>
					<xs:element name="BeginDate" type="xs:date"/>
					<xs:element name="EndDate" type="xs:date"/>
					<xs:element name="TotalInstructionalDays" type="xs:int"/>
					<xs:element name="CalendarDateReference" type="SLC-CalendarDateReferenceType" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="SLC-Assessment">
		<xs:annotation>
			<xs:documentation>Assessment record with key fields: AssessmentTitle, AcademicSubject, GradeLevelAssessed and Version. Changed types of ObjectiveAssessmentReference, SectionReference and
				AssessmentItemReference to SLC reference types.</xs:documentation>
			<xs:appinfo>
				<sli:recordType>assessment</sli:recordType>
				<sli:naturalKeys>AssessmentTitle,AcademicSubject,GradeLevelAssessed,Version</sli:naturalKeys>
			</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="ComplexObjectType">
				<xs:sequence>
					<xs:element name="AssessmentTitle" type="AssessmentTitle"/>
					<xs:element name="AssessmentIdentificationCode" type="AssessmentIdentificationCode" maxOccurs="unbounded"/>
					<xs:element name="AssessmentCategory" type="AssessmentCategoryType" minOccurs="0"/>
					<xs:element name="AcademicSubject" type="SLC-AcademicSubjectType" minOccurs="0"/>
					<xs:element name="GradeLevelAssessed" type="SLC-GradeLevelType" minOccurs="0"/>
					<xs:element name="LowestGradeLevelAssessed" type="SLC-GradeLevelType" minOccurs="0"/>
					<xs:element name="AssessmentPerformanceLevel" type="AssessmentPerformanceLevel" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="ContentStandard" type="SLC-ContentStandardType" minOccurs="0"/>
					<xs:element name="AssessmentForm" type="AssessmentForm" minOccurs="0"/>
					<xs:element name="Version" type="Version" minOccurs="0"/>
					<xs:element name="RevisionDate" type="xs:date" minOccurs="0"/>
					<xs:element name="MaxRawScore" type="xs:int" minOccurs="0"/>
					<xs:element name="Nomenclature" type="Nomenclature" minOccurs="0"/>
					<xs:element name="AssessmentPeriod" type="SLC-AssessmentPeriodDescriptorReferenceType" minOccurs="0"/>
					<xs:element name="AssessmentItemReference" type="AssessmentItemReferenceType" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="ObjectiveAssessmentReference" type="SLC-ObjectiveAssessmentReferenceType" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="AssessmentFamilyReference" type="SLC-AssessmentFamilyReferenceType" minOccurs="0"/>
					<xs:element name="SectionReference" type="SLC-SectionReferenceType" minOccurs="0" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="SLC-AssessmentFamily">
		<xs:annotation>
			<xs:documentation>AssessmentFamily record with key fields: AssessmentFamilyTitle and Version (optional).</xs:documentation>
			<xs:appinfo>
				<sli:recordType>assessmentFamily</sli:recordType>
				<sli:naturalKeys>AssessmentFamilyTitle,Version</sli:naturalKeys>
			</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="ComplexObjectType">
				<xs:sequence>
					<xs:element name="AssessmentFamilyTitle" type="AssessmentTitle"/>
					<xs:element name="AssessmentFamilyIdentificationCode" type="AssessmentIdentificationCode" maxOccurs="unbounded"/>
					<xs:element name="AssessmentCategory" type="AssessmentCategoryType" minOccurs="0"/>
					<xs:element name="AcademicSubject" type="SLC-AcademicSubjectType" minOccurs="0"/>
					<xs:element name="GradeLevelAssessed" type="SLC-GradeLevelType" minOccurs="0"/>
					<xs:element name="LowestGradeLevelAssessed" type="SLC-GradeLevelType" minOccurs="0"/>
					<xs:element name="ContentStandard" type="SLC-ContentStandardType" minOccurs="0"/>
					<xs:element name="Version" type="Version" minOccurs="0"/>
					<xs:element name="RevisionDate" type="xs:date" minOccurs="0"/>
					<xs:element name="Nomenclature" type="Nomenclature" minOccurs="0"/>
					<xs:element name="AssessmentPeriods" type="SLC-AssessmentPeriodDescriptorReferenceType" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="AssessmentFamilyReference" type="SLC-AssessmentFamilyReferenceType" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="SLC-AssessmentPeriodDescriptor">
		<xs:annotation>
			<xs:documentation>This descriptor holds the period or window in which an assessment is supposed to be administered (e.g., Beginning of Year, Middle of Year, End of
				Year).</xs:documentation>
			<xs:appinfo>
				<sli:recordType>assessmentPeriodDescriptor</sli:recordType>
				<sli:naturalKeys>CodeValue</sli:naturalKeys>
			</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="ComplexObjectType">
				<xs:sequence>
					<xs:element name="CodeValue" type="CodeValue"/>
					<xs:element name="Description" type="Description"/>
					<xs:element name="ShortDescription" type="ShortDescription" minOccurs="0"/>
					<xs:element name="BeginDate" type="xs:date" minOccurs="0"/>
					<xs:element name="EndDate" type="xs:date" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="SLC-AssessmentItem">
		<xs:annotation>
			<xs:documentation>AssessmentItem record with key field: IdentificationCode. Changed types of AssessmentReference and LearningStandardReference to SLC reference types.</xs:documentation>
			<xs:appinfo>
				<sli:recordType>assessmentItem</sli:recordType>
				<sli:naturalKeys>IdentificationCode</sli:naturalKeys>
			</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="ComplexObjectType">
				<xs:sequence>
					<xs:element name="AssessmentReference" type="SLC-AssessmentReferenceType"/>
					<xs:element name="IdentificationCode" type="IdentificationCode"/>
					<xs:element name="ItemCategory" type="SLC-ItemCategoryType"/>
					<xs:element name="MaxRawScore" type="xs:int"/>
					<xs:element name="CorrectResponse" type="CorrectResponse"/>
					<xs:element name="LearningStandardReference" type="SLC-LearningStandardReferenceType" minOccurs="0" maxOccurs="unbounded"/>
					<xs:element name="Nomenclature" type="Nomenclature" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="SLC-AttendanceEvent">
		<xs:annotation>
			<xs:documentation>AttendanceEvent record with key fields: StudentReference (StudentUniqueStateId), SchoolReference (StateOrganizationId) and EventDate. Made SchoolReference required.
				Changed types of StudentReference, SchoolReference, SessionReference and SectionReference to SLC reference types.</xs:documentation>
			<xs:appinfo>
				<sli:recordType>attendanceEvent</sli:recordType>
				<sli:naturalKeys>StudentReference,SchoolReference,SchoolYear,EventDate,AttendanceEventCategory</sli:naturalKeys>
			</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="ComplexObjectType">
				<xs:sequence>
					<xs:element name="SchoolYear" type="SchoolYearType"/>
					<xs:element name="EventDate" type="xs:date"/>
					<xs:element name="AttendanceEventType" type="AttendanceEventType" minOccurs="0"/>
					<xs:element name="AttendanceEventCategory" type="SLC-AttendanceEventCategoryType"/>
					<xs:element name="AttendanceEventReason" type="AttendanceEventReason" minOccurs="0"/>
					<xs:element name="EducationalEnvironment" type="SLC-EducationalEnvironmentType" minOccurs="0"/>
					<xs:element name="StudentReference" type="SLC-StudentReferenceType"/>
					<xs:element name="SchoolReference" type="SLC-EducationalOrgReferenceType"/>
					<xs:element name="SessionReference" type="SLC-SessionReferenceType" minOccurs="0"/>
					<xs:element name="SectionReference" type="SLC-SectionReferenceType" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="SLC-BellSchedule">
		<xs:annotation>
			<xs:documentation>Changed types of SessionReference, MeetingTimeReference and CalendarDateReference to SLC reference types.</xs:documentation>
			<xs:appinfo>
				<sli:recordType>bellSchedule</sli:recordType>
			</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="ComplexObjectType">
				<xs:sequence>
					<xs:element name="BellScheduleName" type="BellScheduleName"/>
					<xs:element name="GradeLevels" type="SLC-GradeLevelsType"/>
					<xs:element name="WeeksInCycle">
						<xs:simpleType>
							<xs:restriction base="xs:int">
								<xs:minInclusive value="1"/>
								<xs:maxInclusive value="52"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="SessionReference" type="SLC-SessionReferenceType"/>
					<xs:element name="MeetingTimeReference" type="SLC-MeetingTimeReferenceType" maxOccurs="unbounded"/>
					<xs:element name="CalendarDateReference" type="SLC-CalendarDateReferenceType" minOccurs="0" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="SLC-CalendarDate">
		<xs:annotation>
			<xs:documentation>CalendarDate record with key fields: Date and EducationOrgReference (StateOrganizationId). Added EducationOrgReference.</xs:documentation>
			<xs:appinfo>
				<sli:recordType>calendarDate</sli:recordType>
				<sli:naturalKeys>Date,EducationOrgReference</sli:naturalKeys>
			</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="ComplexObjectType">
				<xs:sequence>
					<xs:element name="Date" type="xs:date"/>
					<xs:element name="CalendarEvent" type="CalendarEventType"/>
					<xs:element name="EducationOrgReference" type="SLC-EducationalOrgReferenceType"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="SLC-Cohort">
		<xs:annotation>
			<xs:documentation>Cohort record with key fields: EducationOrgReference (StateOrganizationId) and CohortIdentifier. Changed types of EducationOrgReference and ProgramReference to SLC
				reference types.</xs:documentation>
			<xs:appinfo>
				<sli:recordType>cohort</sli:recordType>
				<sli:naturalKeys>EducationOrgReference,CohortIdentifier</sli:naturalKeys>
			</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="ComplexObjectType">
				<xs:sequence>
					<xs:element name="CohortIdentifier" type="CohortIdentifier"/>
					<xs:element name="CohortDescription" type="CohortDescription" minOccurs="0"/>
					<xs:element name="CohortType" type="CohortType"/>
					<xs:element name="CohortScope" type="CohortScopeType" minOccurs="0"/>
					<xs:element name="AcademicSubject" type="SLC-AcademicSubjectType" minOccurs="0"/>
					<xs:element name="EducationOrgReference" type="SLC-EducationalOrgReferenceType"/>
					<xs:element name="ProgramReference" type="SLC-ProgramReferenceType" minOccurs="0" maxOccurs="unbounded"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="SLC-CompetencyLevelDescriptor">
		<xs:annotation>
			<xs:documentation>This descriptor defines various levels for assessed competencies.</xs:documentation>
			<xs:appinfo>
				<sli:recordType>competencyLevelDescriptor</sli:recordType>
				<sli:naturalKeys>CodeValue</sli:naturalKeys>
			</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="ComplexObjectType">
				<xs:sequence>
					<xs:element name="CodeValue" type="CodeValue"/>
					<xs:element name="Description" type="Description" minOccurs="0"/>
					<xs:element name="PerformanceBaseConversion" type="PerformanceBaseType" minOccurs="0"/>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>
	<xs:complexType name="SLC-Course">
		<xs:annotation>
			<xs:documentation>Course record with key fields: EducationOrganizationReference (StateOrganizationId) and UniqueCourseId. Changed types of EducationOrganizationReference,
				LearningStandardReference and LearningObjectiveReference to SLC reference types.</xs:documentation>
			<xs:appinfo>
				<sli:recordType>course</sli:recordType>
				<sli:naturalKeys>EducationOrganizationReference,UniqueCourseId</sli:naturalKeys>
			</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base="ComplexObjectType">
				<xs:sequence>
					<xs:element name="CourseTitle" type="CourseTitle"/>
					<xs:element name="NumberOfParts">
						<xs:simpleType>
							<xs:restriction base="xs:int">
								<xs:minInclusive value="1"/>
								<xs:maxInclusive value="8"/>
							</xs:restriction>
						</xs:simpleType>
					</xs:element>
					<xs:element name="CourseCode" type="CourseCode" maxOccurs="unbounded"/>
					<xs:element name="CourseLevel" type="CourseLevelType" minOccurs="0"/>
					<xs:element name="CourseLevelCharacteristics" type="CourseLevelCharacteristicsType" minOccurs="0"/>
					<xs:element name="GradesOffered" type="SLC-GradeLevelsType" minOccurs="0"/>
					<xs:element name="SubjectArea" type="SLC-AcademicSubjectType" minOccurs="0"/>
					<xs:element name="CourseDescription" type="Description" minOccurs="0"/>
					<xs:element name="DateCourseAdopted" type="xs:date" minOccurs="0"/>
					<xs:element name="HighSchoolCourseRequirement" type="xs:boolean" minOccurs="0"/>
					<xs:element name="CourseGPAApplicability" type="CourseGPAApplicabilityType" minOccurs="0"/>
					<xs:element name="CourseDefinedBy" type="CourseDefinedByType" minOccurs="0"/>
