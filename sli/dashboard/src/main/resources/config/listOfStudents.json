{
  id : "listOfStudents",
  type : "PANEL",
  data :{
    lazy: true,
    entity: "listOfStudents",
    alias: "listOfStudents",
    params: {
      assessmentFilter: {"ISAT Reading": "MOST_RECENT_RESULT", 
                         "ISAT Writing": "MOST_RECENT_RESULT", 
                         "DIBELS Next": "MOST_RECENT_WINDOW",
                         "TRC": "MOST_RECENT_WINDOW",
                         "SAT Reading": "HIGHEST_EVER",
                         "SAT Writing": "HIGHEST_EVER",
                         "AP Language": "HIGHEST_EVER",
                         "AP Literature": "HIGHEST_EVER" }
    }
  },
  root: 'students',
  items : [
    {name: "Middle School ELA View", 
     condition: {field: "gradeLevels", value: ["Third grade", "Fourth grade", "Fifth grade", "Sixth grade", "Seventh grade", "Eighth grade"]},
     items: [
      {name: "Student", width: 150, field: "name.fullName", formatter:'showlink', params: {baseLinkUrl:'student', idName:'id', target:"_blank", showAction=""}},
      {name: "", width: 60, field: "programParticipation", formatter: DashboardUtil.Grid.Formatters.Lozenge},
      {name: "ISAT Reading", items:[
        {name: "Perf. Lvl.", field: "assessments.ISAT Reading.perfLevel", width:100, sorter: 'int'},
        {name: "SS", field: "assessments.ISAT Reading.Scale score", width:100, sorter: 'int'},
        {name: "Lexile Score", field: "assessments.ISAT Reading.Other", width:100}]},
      {name: "ISAT Writing (most recent)", items:[   
        {name: "Perf. Lvl.", field: "assessments.ISAT Writing.perfLevel", width:100, sorter: 'int'},
        {name: "SS", field: "assessments.ISAT Writing.Scale score", width:100, sorter: 'int'}]},
      {name: "Attendance (current school year)", items:[   
        {name: "Absence Count", field: "attendances.absenceCount", width:100, sorter: 'int', formatter: DashboardUtil.Grid.Formatters.CutPointReverse, params:{cutPoints:{0:{style:'color-widget-darkgreen'}, 1:{style:'color-widget-green'}, 6:{style: 'color-widget-yellow'}, 11:{style:'color-widget-red'}}}},
        {name: "Attendance Rate %", field: "attendances.attendanceRate", width:100, sorter: 'int', formatter: DashboardUtil.Grid.Formatters.CutPoint, params:{cutPoints:{89:{style:'color-widget-red'}, 94:{style: 'color-widget-yellow'}, 98:{style:'color-widget-green'}, 100:{style:'color-widget-darkgreen'}}}},
        {name: "Tardy Count", field: "attendances.tardyCount", width:100, sorter: 'int', formatter: DashboardUtil.Grid.Formatters.CutPointReverse, params:{cutPoints:{0:{style:'color-widget-darkgreen'}, 1:{style: 'color-widget-green'}, 6:{style:'color-widget-yellow'}, 11:{style:'color-widget-red'}}}},
        {name: "Tardy Rate %", field: "attendances.tardyRate", width:100, sorter: 'int', formatter: DashboardUtil.Grid.Formatters.CutPointReverse, params:{cutPoints:{0:{style:'color-widget-darkgreen'}, 1:{style: 'color-widget-green'}, 6:{style:'color-widget-yellow'}, 11:{style:'color-widget-red'}}}}]}
    ]},
    {name: "Early Literacy View",
     condition: {field: "gradeLevels", value: ["Kindergarten", "First grade", "Second grade", "Third grade"]},
     items: [
      {name: "Student", field: "name.fullName", width:150},
      {name: "", width: 60, field: "programParticipation", formatter: DashboardUtil.Grid.Formatters.Lozenge},
      {name: "DIBELS Next", items:[
        {name: "Perf. Lvl.", field: "assessments.DIBELS Next.perfLevel", width:100}]},
      {name: "Reading", items:[          
        {name: "RL", field: "assessments.TRC.readingLevel", width:100},
        {name: "Prof. Level", field: "assessments.TRC.profLevel", width:100}]},
      {name: "Attendance", items:[   
        {name: "Absence Count", field: "attendances.absenceCount", sorter: 'int', width:100, formatter: DashboardUtil.Grid.Formatters.CutPointReverse, params:{cutPoints:{0:{style:'color-widget-darkgreen'}, 1:{style: 'color-widget-green'}, 6:{style: 'color-widget-yellow'}, 11:{style: 'color-widget-red'}}}},
        {name: "Tardy Count", field: "attendances.tardyCount", sorter: 'int', width:100, formatter: DashboardUtil.Grid.Formatters.CutPointReverse, params:{cutPoints:{0:{style:'color-widget-darkgreen'}, 1:{style: 'color-widget-green'}, 6:{style: 'color-widget-yellow'}, 11:{style: 'color-widget-red'}}}}]}
    ]},
    {name: "College Ready ELA View", 
     condition: {field: "gradeLevels", value: ["Ninth grade", "Tenth grade", "Eleventh grade", "Twelfth grade"]},
     items: [
      {name: "Student", field: "name.fullName", width:150},
      {name: "", width: 60, field: "programParticipation", formatter: DashboardUtil.Grid.Formatters.Lozenge},
      {name: "Reading Test Scores (Highest)", items:[
        {name: "SAT", field: "assessments.SAT Reading.x", width:100, sorter: 'int'},
        {name: "%ile", field: "assessments.SAT Reading.percentile", width:100, sorter: 'int'}]},   
      {name: "Writing Test Scores (Highest)", items:[          
        {name: "SAT", field: "assessments.SAT Writing.x", width:100, sorter: 'int'},
        {name: "%ile", field: "assessments.SAT Writing.percentile", width:100, sorter: 'int'}]},
      {name: "AP Eng. Exam Scores (Highest)", items:[          
        {name: "Lang.", field: "assessments.AP Language.x", width:100},
        {name: "Lit.", field: "assessments.AP Literature.x", width:100}]},                   
      {name: "Attendance", items:[   
        {name: "Absence Count", field: "attendances.absenceCount", width: 100, sorter: 'int', formatter: DashboardUtil.Grid.Formatters.CutPointReverse, params:{cutPoints:{0:{style:'color-widget-darkgreen'}, 1:{style:'color-widget-green'}, 6:{style: 'color-widget-yellow'}, 11:{style: 'color-widget-red'}}}},
        {name: "Attendance Rate %", field: "attendances.attendanceRate", width: 100, sorter: 'int', formatter: DashboardUtil.Grid.Formatters.CutPoint, params:{cutPoints:{89:{style: 'color-widget-red'}, 94:{style:'color-widget-yellow'}, 98:{style:'color-widget-green'}, 100:{style: 'color-widget-darkgreen'}}}},
        {name: "Tardy Count", field: "attendances.tardyCount", width: 100, sorter: 'int', formatter: DashboardUtil.Grid.Formatters.CutPointReverse, params:{cutPoints:{0:{style:'color-widget-darkgreen'}, 1:{style:'color-widget-green'}, 6:{style:'color-widget-yellow'}, 11:{style: 'color-widget-red'}}}},
        {name: "Tardy Rate %", field: "attendances.tardyRate", width: 100, sorter: 'int', formatter: DashboardUtil.Grid.Formatters.CutPointReverse, params:{cutPoints:{0:{style:'color-widget-darkgreen'}, 1:{style:'color-widget-green'}, 6:{style:'color-widget-yellow'}, 11:{style: 'color-widget-red'}}}}]}
    ]}
  ] 
}