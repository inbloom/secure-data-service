	<xs:complexType name='grade'>
		<xs:annotation>
			<xs:documentation>This educational entity represents an overall score
				or assessment tied to a course over a period of time (i.e., the
				grading period). Student grades are usually a compilation of marks
				and other scores.
			</xs:documentation>
			<xs:appinfo>
				<sli:CollectionType>grade</sli:CollectionType>
				<sli:applyNaturalKeys>true</sli:applyNaturalKeys>
				<sli:schemaVersion>1</sli:schemaVersion>
                <sli:associatedDatedCollection>studentSectionAssociation</sli:associatedDatedCollection>
			</xs:appinfo>
		</xs:annotation>
		<xs:sequence>
			<xs:element minOccurs='0' name='letterGradeEarned'>
				<xs:annotation>
					<xs:documentation>A final or interim (grading period) indicator
						of
						student performance in a class as submitted by the
						instructor.
					</xs:documentation>
					<xs:appinfo>
						<sli:WriteEnforcement><sli:allowedBy>WRITE_GENERAL</sli:allowedBy></sli:WriteEnforcement>
						<sli:ReadEnforcement><sli:allowedBy>READ_GENERAL</sli:allowedBy></sli:ReadEnforcement>
					</xs:appinfo>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base='gradeEarned'>
						<xs:maxLength value='20' />
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element minOccurs='0' name='numericGradeEarned' type='xs:integer'>
				<xs:annotation>
					<xs:documentation>A final or interim (grading period) indicator
						of
						student performance in a class as submitted by the
						instructor.
					</xs:documentation>
					<xs:appinfo>
						<sli:WriteEnforcement><sli:allowedBy>WRITE_GENERAL</sli:allowedBy></sli:WriteEnforcement>
						<sli:ReadEnforcement><sli:allowedBy>READ_GENERAL</sli:allowedBy></sli:ReadEnforcement>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element minOccurs='0' name='diagnosticStatement' type='text'>
				<xs:annotation>
					<xs:documentation>A statement provided by the teacher that
						provides
						information in addition to the grade or assessment
						score.
					</xs:documentation>
					<xs:appinfo>
						<sli:WriteEnforcement><sli:allowedBy>WRITE_GENERAL</sli:allowedBy></sli:WriteEnforcement>
						<sli:ReadEnforcement><sli:allowedBy>READ_GENERAL</sli:allowedBy></sli:ReadEnforcement>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name='gradeType' type='gradeType'>
				<xs:annotation>
					<xs:documentation>The type of grade reported (e.g., Exam, Final,
						Grading Period).
					</xs:documentation>
					<xs:appinfo>
						<sli:WriteEnforcement><sli:allowedBy>WRITE_GENERAL</sli:allowedBy></sli:WriteEnforcement>
						<sli:ReadEnforcement><sli:allowedBy>READ_GENERAL</sli:allowedBy></sli:ReadEnforcement>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element minOccurs='0' name='performanceBaseConversion'
				type='performanceBaseType'>
				<xs:annotation>
					<xs:documentation>A conversion of the level to a standard set of
						performance levels.
					</xs:documentation>
					<xs:appinfo>
						<sli:WriteEnforcement><sli:allowedBy>WRITE_GENERAL</sli:allowedBy></sli:WriteEnforcement>
						<sli:ReadEnforcement><sli:allowedBy>READ_GENERAL</sli:allowedBy></sli:ReadEnforcement>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element name='studentSectionAssociationId' type='reference'>
				<xs:annotation>
					<xs:documentation>Association to the student-section association
						with which the grade is awarded.
					</xs:documentation>
					<xs:appinfo>
						<sli:ReferenceType>studentSectionAssociation</sli:ReferenceType>
						<sli:naturalKey>true</sli:naturalKey>
						<sli:WriteEnforcement><sli:allowedBy>WRITE_GENERAL</sli:allowedBy></sli:WriteEnforcement>
						<sli:ReadEnforcement><sli:allowedBy>READ_GENERAL</sli:allowedBy></sli:ReadEnforcement>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element minOccurs='0' name='gradingPeriodId' type='reference'>
				<xs:annotation>
					<xs:documentation>Identifies the grading period for the grade.
					</xs:documentation>
					<xs:appinfo>
						<sli:ReferenceType>gradingPeriod</sli:ReferenceType>
						<sli:naturalKey>true</sli:naturalKey>
						<sli:WriteEnforcement><sli:allowedBy>WRITE_GENERAL</sli:allowedBy></sli:WriteEnforcement>
						<sli:ReadEnforcement><sli:allowedBy>READ_GENERAL</sli:allowedBy></sli:ReadEnforcement>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>

