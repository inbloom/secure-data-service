	<xs:complexType name='teacher'>
		<xs:annotation>
			<xs:documentation>This entity represents a person that is employed by
				an LEA or other educational unit engaged in student instruction.
				These persons are instructional-type staff members. In the data
				model, a teacher entity is a staff member with additional
				properties.
			</xs:documentation>
			<xs:appinfo>
				<sli:CollectionType>staff</sli:CollectionType>
				<sli:applyNaturalKeys>true</sli:applyNaturalKeys>
				<sli:schemaVersion>1</sli:schemaVersion>
			</xs:appinfo>
		</xs:annotation>
		<xs:complexContent>
			<xs:extension base='staff'>
				<xs:sequence>
					<xs:element minOccurs='0' name='teacherUniqueStateId'
						type='uniqueStateIdentifier'>
						<xs:annotation>
							<xs:documentation>An ID number maintained by the state that
								uniquely
								identifies this teacher.
							</xs:documentation>
							<xs:appinfo>
								<sli:ReadEnforcement><sli:allowedBy>READ_RESTRICTED</sli:allowedBy></sli:ReadEnforcement>
								<sli:WriteEnforcement><sli:allowedBy>WRITE_RESTRICTED</sli:allowedBy></sli:WriteEnforcement>
								<sli:naturalKey>false</sli:naturalKey>
							</xs:appinfo>
						</xs:annotation>
					</xs:element>
					<xs:element minOccurs='0' name='highlyQualifiedTeacher'
						type='xs:boolean'>
						<xs:annotation>
							<xs:documentation>An indication of whether a teacher is
								classified as highly qualified for his/her assignment according
								to state definition. This attribute indicates the teacher is
								highly qualified for ALL sections being taught.
							</xs:documentation>
							<xs:appinfo>
								<sli:ReadEnforcement><sli:allowedBy>READ_RESTRICTED</sli:allowedBy></sli:ReadEnforcement>
								<sli:WriteEnforcement><sli:allowedBy>WRITE_RESTRICTED</sli:allowedBy></sli:WriteEnforcement>
							</xs:appinfo>
						</xs:annotation>
					</xs:element>
				</xs:sequence>
			</xs:extension>
		</xs:complexContent>
	</xs:complexType>

	<xs:complexType name='teacherSchoolAssociation'>
