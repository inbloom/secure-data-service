					<xs:appinfo>
						<sli:WriteEnforcement><sli:allowedBy>WRITE_GENERAL</sli:allowedBy></sli:WriteEnforcement>
						<sli:ReadEnforcement><sli:allowedBy>READ_GENERAL</sli:allowedBy></sli:ReadEnforcement>
                        <sli:endDate>yes</sli:endDate>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
			<xs:element minOccurs='0' name='highlyQualifiedTeacher'
				type='xs:boolean'>
				<xs:annotation>
					<xs:documentation>An indication of whether a teacher is classified
						as highly
						qualified for his/her assignment according to state
						definition. This
						attribute indicates the teacher is highly
						qualified for this section being
						taught.
					</xs:documentation>
					<xs:appinfo>
						<sli:WriteEnforcement><sli:allowedBy>WRITE_GENERAL</sli:allowedBy></sli:WriteEnforcement>
						<sli:ReadEnforcement><sli:allowedBy>READ_GENERAL</sli:allowedBy></sli:ReadEnforcement>
					</xs:appinfo>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
